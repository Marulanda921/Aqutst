{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "GroupId": "Aqutest-group",
    "AutoOffsetReset": "earliest",
    "Topic": "Aqua-topic"
  },
  "TCP": {
    "Port": "8080"
  },
  "MongoDB": {
    "Url": "mongodb://localhost:27017",
    "Database": "Aqutest"
  }
}
